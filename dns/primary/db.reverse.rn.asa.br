; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	rn.asa.br. root.rn.asa.br. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	rn.asa.br.
@	IN	NS	ns1.rn.asa.br.
;@	IN	NS	ns2.rn.asa.br.

reverse1	IN	PTR	rn.asa.br
reverse1	IN	PTR	ns1
reverse1	IN	PTR	mail
;reverse2	IN	PTR	ns2
